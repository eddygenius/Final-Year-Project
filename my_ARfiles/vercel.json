{
  "version": 2,
  "public": true,
  "builds": [
    { "src": "my_ARfiles/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/my_ARfiles/(.*)", "dest": "/my_ARfiles/$1" },
    { "src": "/libs/(.*)", "dest": "/my_ARfiles/libs/$1" },
    { "src": "/assets/(.*)", "dest": "/my_ARfiles/assets/$1" },
    { "src": "/complete/(.*)", "dest": "/my_ARfiles/complete/$1" },
    { "src": "/app.js", "dest": "/my_ARfiles/complete/main-file/app.js" },
    { "src": "/", "dest": "/my_ARfiles/complete/main-file/index.html" }
  ]
}
